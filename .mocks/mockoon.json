        {
          "uuid": "c6d5e4f3-2a1b-9c8d-7e6f-5a4b3c2d1e0f",
          "body": "{\n  \"data_fim_plantio\": \"{{add (body 'data_inicio_plantio') (body 'planta.dias_maturidade') 'days'}}\",\n  \"descritivo de como plantar\": \"Para plantar o {{body 'planta.nome_cientifico'}}, escolha um local ensolarado no seu jardim ou em um vaso. Prepare o solo, garantindo que ele esteja bem drenado e fértil. Plante as sementes a uma profundidade de 1 cm e mantenha o solo úmido, mas não encharcado. Após cerca de {{body 'planta.dias_maturidade'}} dias, você poderá colher as folhas frescas.\",\n  \"informacoes adicionais\": \"O {{body 'planta.nome_cientifico'}} é uma planta aromática que se adapta bem a climas quentes. Certifique-se de regá-lo regularmente e evite deixá-lo exposto a temperaturas muito baixas. Pode ser cultivado em vasos ou diretamente no solo do jardim.\",\n  \"tarefas\": [\n    {\n      \"nome\": \"Regar o {{body 'planta.nome_cientifico'}}\",\n      \"tipo\": \"cuidados\",\n      \"quantidade_total\": {{body 'planta.dias_maturidade'}},\n      \"cron\": \"0 7 * * *\",\n      \"habilidade\": {\n        \"nome\": \"nome da habilidade\",\n        \"multiplicador_xp\": 1.1\n      }\n    },\n    {\n      \"nome\": \"Adubar o {{body 'planta.nome_cientifico'}}\",\n      \"tipo\": \"cuidados\",\n      \"quantidade_total\": {{divide (body 'planta.dias_maturidade') 15}},\n      \"cron\": \"0 7 * * 0\",\n      \"habilidade\": {\n        \"nome\": \"nome da habilidade\",\n        \"multiplicador_xp\": 1.2\n      }\n    },\n    {\n      \"nome\": \"Verificar pragas no {{body 'planta.nome_cientifico'}}\",\n      \"tipo\": \"cuidados\",\n      \"quantidade_total\": {{divide (body 'planta.dias_maturidade') 10}},\n      \"cron\": \"0 7 * * 3\",\n      \"habilidade\": {\n        \"nome\": \"nome da habilidade\",\n        \"multiplicador_xp\": 1.3\n      }\n    },\n    {\n      \"nome\": \"Colher o {{body 'planta.nome_cientifico'}}\",\n      \"tipo\": \"colheita\",\n      \"quantidade_total\": 1,\n      \"cron\": null,\n      \"habilidade\": {\n        \"nome\": \"nome da habilidade\",\n        \"multiplicador_xp\": 2.0\n      }\n    }\n  ]\n}",
